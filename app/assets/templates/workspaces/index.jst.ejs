<div class="container">
  <img class="landing-page-img" src="optimized-pebbles3-editted.jpg" alt="" />
  <div class="row">
    <div class="col-lg-12">
      <h1 class="page-header"><%= TmUp.CURRENT_USER.fname %>'s Workspaces</h1>
    </div>
  </div>

  <div class="row">
    <ul>
      <div class="col-md-3 portfolio-item new-workspace"
           data-step="1"
           data-intro="Welcome to TmUp!! You can create new workspace by clicking on the plus sign below or visit one of the already created workspaces by clicking on them."
           data-position="top">
        <a class="img-responsive">
          <h2>&plus;</h2>
        </a>
      </div>
      <% workspaces.each(function (workspace) { %>
        <div class="col-md-3 portfolio-item">
          <a class="img-responsive" href="#/workspaces/<%= workspace.id %>">
            <h2><%= workspace.escape("title") %></h2>
          </a>
        </div>
      <% }) %>
    </ul>
  </div>
</div>
